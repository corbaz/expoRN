{
  "name": "expotw",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "nvm-node18": "nvm use 18",
    "nvm-node18-bash": "bash -c 'source ~/.nvm/nvm.sh && nvm use 18'",
    "doctor": "bunx -y expo-doctor@latest",
    "check-update": "bunx expo install --check",
    "cloudflared": "cloudflared tunnel --url http://localhost:8081",
    "npn-install": "npm install",
    "pnpn-install": "pnpm install",
    "bun-install": "bun install",
    "start": "expo start",
    "server": "cd backend && bun run index.ts",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.1.3",
    "@types/react": "~18.2.45",
    "expo": "~50.0.14",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-web": "~0.19.6",
    "twrnc": "^4.2.0",
    "typescript": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
  "private": true
}
