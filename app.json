{
  "expo": {
    "name": "expoTW",
    "slug": "expoTW",
    "scheme": "myapp",
    "version": "1.0.0",
    "description": "Expo con Tailwind",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/adaptive-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ed0190"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jcc.expoTW"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ed0190"
      },
      "package": "com.jcc.expoTW"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8047c6c3-93c1-4312-85ef-12a64fa43dc9"
      }
    },
    "plugins": [
      "expo-router"
    ]
  }
}
