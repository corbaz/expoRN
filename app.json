{
    "expo": {
        "name": "expoTW",
        "slug": "expoTW",
        "version": "1.0.0",
        "description": "Expo con Tailwind",
        "orientation": "portrait",
        "icon": "./src/assets/icon.png",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./src/assets/adaptive-icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#ed0190"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.jcc.expoTW"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./src/assets/adaptive-icon.png",
                "backgroundColor": "#ed0190"
            }
        },
        "web": {
            "favicon": "./src/assets/favicon.png"
        }
    }
}
